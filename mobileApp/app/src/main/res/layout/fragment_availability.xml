<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/olive_background"
    tools:context=".ui.navigationfragments.availability.AvailabilityFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/availabilityRecyclerView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="65dp">

    </androidx.recyclerview.widget.RecyclerView>

    <ImageButton
        android:id="@+id/addNewAvailabilityButton"
        android:layout_width="90dp"
        android:layout_height="125dp"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="560dp"
        android:background="@null"
        android:src="@drawable/add_circle_bottom_nav"/>

</FrameLayout>
